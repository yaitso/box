{
    "mcpServers": {
        "chrome-devtools": {
            "command": "bunx",
            "args": [
                "chrome-devtools-mcp@latest",
                "--browserUrl=http://127.0.0.1:9222",
                "--viewport=1536x768"
            ]
        },
        "playwright": {
            "command": "npx",
            "args": [
                "@playwright/mcp@latest",
                "--viewport-size=1536x768"
            ]
        }
    }
}
